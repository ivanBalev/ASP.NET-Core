@model BDInSelfLove.Web.ViewComponents.Models.Video.AllHomeVideoViewModel
@{
    int index = 0;
}

@foreach (var video in Model.Videos)
{
    <div class="carousel-item @(index == 0 ? "active" : "")" itemscope itemtype="http://schema.org/Movie">
        <div class="embed-responsive embed-responsive-16by9">
            <iframe title="Home psychotherapy video" width="560" height="315" src="@video.Url" frameborder="0"
                    allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
                    allowfullscreen>
            </iframe>
        </div>
    </div>
    @(index++);
}