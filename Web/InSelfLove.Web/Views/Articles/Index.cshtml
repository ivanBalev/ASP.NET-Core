@model InSelfLove.Web.ViewModels.Article.ArticlesPaginationViewModel
@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer localizer

@{
    var title = @localizer["Articles"].Value;
    // Provide title to layout partial for title header
    ViewData["Title"] = title;
}

<partial name="_SlimShowcasePartial" model="@title" />

@*_ArticlesAllPartial exists because it's used 2 controllers - Articles & Search *@
<partial name="_ArticlesAllPartial" model="@Model" />
